<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean class="com.pack1.Student" name="student1">
		<property name="id" value="112"></property>
		<property name="name" value="Manish"></property>
		<property name="addr" ref="addr1"></property>
	</bean>

	<bean class="com.pack1.Address" name="addr1">
		<property name="pincode" value="110001"></property>
		<property name="city" value="Delhi"></property>
	</bean>

</beans>
