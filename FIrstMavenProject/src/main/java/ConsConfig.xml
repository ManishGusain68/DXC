<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean class="com.pack1.Student" name="std1">
		<constructor-arg value="112" />
		<constructor-arg value="Sanjeeta" />
		<constructor-arg ref="addr1" />
	</bean>

	<bean class="com.pack1.Address" name="addr1">
		<constructor-arg value="110099" />
		<constructor-arg value="Guntur" />
	</bean>

</beans>