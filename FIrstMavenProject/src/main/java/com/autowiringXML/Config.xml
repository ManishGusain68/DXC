<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context.xsd">


	<bean class="com.autowiringXML.Address" name="addrree">
		<property name="pincode" value="119900" />
		<property name="name" value="agra"/>
	</bean>
	<!-- bean definitions here -->
	<bean class="com.autowiringXML.Student" name="std1" autowire="byName">
		<property name="id" value="112" />
		<property name="name" value="Manish"/>
	</bean>



</beans>